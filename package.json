{
  "name": "@gahojin-inc/aws-lambda-mock-context",
  "version": "2024.10.3",
  "description": "AWS Lambda mock context object",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/aws-lambda-mock-context.git"
  },
  "files": ["dist"],
  "keywords": ["aws", "lambda", "mock"],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "prepare": "husky",
    "clean": "rimraf dist",
    "build": "tsup",
    "prepublish": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsc",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage",
    "ncu": "npm-check-updates -u"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "dependencies": {
    "uuid": "11.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@types/aws-lambda": "8.10.145",
    "@types/node": "20.17.6",
    "@types/uuid": "10.0.0",
    "@vitest/coverage-v8": "2.1.5",
    "husky": "9.1.7",
    "jest-extended": "4.0.2",
    "npm-check-updates": "17.1.11",
    "rimraf": "6.0.1",
    "tsup": "8.3.5",
    "typescript": "5.6.3",
    "vite-tsconfig-paths": "5.1.2",
    "vitest": "2.1.5"
  },
  "peerDependencies": {
    "uuid": ">=8.0.0"
  },
  "packageManager": "pnpm@9.13.2",
  "engines": {
    "node": ">=18.x",
    "npm": "use pnpm",
    "pnpm": ">=9.x",
    "yarn": "use pnpm"
  }
}
