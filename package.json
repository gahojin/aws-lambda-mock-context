{
  "name": "@gahojin-inc/aws-lambda-mock-context",
  "version": "2025.6.1",
  "description": "AWS Lambda mock context object",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/aws-lambda-mock-context.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "aws",
    "lambda",
    "mock"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "preinstall": "pnpm dlx only-allow pnpm",
    "clean": "rimraf dist",
    "prebuild": "rimraf dist",
    "build": "rolldown -c",
    "prepublish": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsc",
    "check:packagejson": "attw --pack .",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage",
    "ncu": "npm-check-updates -u"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "2.0.6",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@types/aws-lambda": "8.10.150",
    "@types/node": "22.16.0",
    "@vitest/coverage-v8": "3.2.4",
    "jest-extended": "6.0.0",
    "lefthook": "1.11.14",
    "npm-check-updates": "18.0.1",
    "rimraf": "6.0.1",
    "rolldown": "1.0.0-beta.23",
    "typescript": "5.8.3",
    "unplugin-isolated-decl": "0.14.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  },
  "packageManager": "pnpm@10.12.4",
  "engines": {
    "node": ">=18.x",
    "npm": "use pnpm",
    "pnpm": ">=9.x",
    "yarn": "use pnpm"
  }
}
