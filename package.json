{
  "name": "@gahojin-inc/aws-lambda-mock-context",
  "version": "2025.10.1",
  "description": "AWS Lambda mock context object",
  "author": "GAHOJIN, Inc.",
  "license": "Apache-2.0",
  "type": "module",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/gahojin/aws-lambda-mock-context.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "aws",
    "lambda",
    "mock"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "rimraf dist",
    "build": "rolldown -c",
    "prepublishOnly": "pnpm run build",
    "lint": "biome check --write .",
    "check": "tsc",
    "check:packagejson": "attw --pack .",
    "test": "vitest --watch",
    "test:unit": "vitest --passWithNoTests --run --coverage",
    "ncu": "npm-check-updates -u"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "0.18.2",
    "@biomejs/biome": "2.2.7",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@types/aws-lambda": "8.10.156",
    "@types/node": "22.18.12",
    "@vitest/coverage-v8": "4.0.1",
    "jest-extended": "6.0.0",
    "lefthook": "2.0.0",
    "npm-check-updates": "19.1.1",
    "rimraf": "6.0.1",
    "rolldown": "1.0.0-beta.44",
    "typescript": "5.9.3",
    "unplugin-isolated-decl": "0.15.2",
    "vite": "7.1.11",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.1"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "lefthook"
    ]
  }
}
